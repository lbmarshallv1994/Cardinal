[%-
    PROCESS "opac/parts/css/colors.tt2";
    PROCESS "opac/parts/css/fonts.tt2";
    rtl = ctx.get_i18n_l(ctx.eg_locale).rtl;
    header_height = "58px";
    header_logo_size = "45px";
    header_mobile_height = "48px";
    header_mobile_logo_size = "35px";
    header_dash_width = "575px";
    header_dash_inactive_width = "185px";
    tablet_dash_width = "372px";
    mobile_dash_width = "48px";
    link_bar_height = "22px";
    result_image_width = "120px";
    result_facet_width = "20vw";
%]

@font-face {
  font-family: 'Lato';
  src:
    url([% ctx.media_prefix %]/opac/extras/fonts/Lato-Regular.ttf) format('truetype')
  ;
  font-style: normal;
  font-weight: normal;
}
@font-face {
  font-family: 'Lato';
  src:
    url([% ctx.media_prefix %]/opac/extras/fonts/Lato-Italic.ttf) format('truetype')
  ;
  font-style: italic;
  font-weight: normal;
}

/* Bold */
@font-face {
  font-family: 'Lato';
  src:
    url([% ctx.media_prefix %]/opac/extras/fonts/Lato-Bold.ttf) format('truetype')
  ;
  font-style: normal;
  font-weight: 700;
}
@font-face {
  font-family: 'Lato';
  src:
    url([% ctx.media_prefix %]/opac/extras/fonts/Lato-BoldItalic.ttf) format('truetype')
  ;
  font-style: italic;
  font-weight: 700;
}

body {
    margin:0;
    font-family: Lato, Arial, Helvetica, sans-serif;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    text-rendering: optimizeLegibility;
    font-size: [% css_fonts.size_base %];
    background-color: [% css_colors.primary %];
    [% IF rtl == 't' -%]
    direction: rtl;
    [%- END %]
}

button, input {
    font-family: Lato, Arial, Helvetica, sans-serif;
    font-size: [% css_fonts.size_base %];
}

img {
    border: none;
}

input{
    display: inline-block;
}

select{
    display: inline-block;
}

h3 {
    font-size: [% css_fonts.size_bigger %];
}

h4 {
    font-size: [% css_fonts.size_big %];
}

.form-control{
    display: inline-block;
    width: auto;
    border-color: [% css_colors.border_input %];
    color: [% css_colors.text_input %];
    font-size: [% css_fonts.size_base %];
    margin: 0.1em;
    
}

@supports(-webkit-appearance: none) {
    select.form-control{
        -webkit-appearance: none;
            
    }

    /* dropdown arrow only appears on selects that are one row */
    select.form-control:not([size]){
       background: #fff url("[% ctx.media_prefix %]/images/gray-arrow-down.png") no-repeat 95% 50%; 
    }
}

select.form-control:focus{
    outline: none;
}

.form-check-inline{
    margin-right: 0;
}

.flexbox{
    display: flex;
    flex-wrap: wrap;
    align-items: center;
}


#topnav_logo_wrapper{
    display: inline-flex;
    align-items: center;
    height: 100%;
}

#header:not(.user-active) #header-login-wrapper{
    width: [% header_dash_inactive_width %];
}

