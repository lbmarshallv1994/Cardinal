}

h2.graphic880 {
    font-size: [% css_fonts.size_bigger %];
    font-weight:normal;
}

.hide_me, .hidden {
    display: none;
    visibility: hidden;
}

.modal-header{
    background-color: [% css_colors.primary_fade %];
    color: [% css_colors.text_invert %];
    padding: 0.6rem 0.6rem;
}

.modal-footer {
    padding: .10rem;
}

div.select-box-wrapper {
    position:absolute;
    padding-top:2px;
    overflow:hidden;
    [% IF rtl == 't' -%]
    padding-right: 3px;
    text-align:right;
    [%- ELSE %]
